{
	"name": "<%= _.slugify(_.humanize(appname)) %>",
	"description": "<%= pkg.description %>",
	"version": "0.0.0",
	"private": true,
	"dependencies": {
		"purecss": "yahoo/pure#0.6.0"<% if (hasFeat) { %>,<% } if (includeModernizr) { %>
		"modernizr": "Modernizr/Modernizr#2.8.3"<% if (includejQuery || includeSizzle) {%>,<%}} else { if (includeHTML5Shiv) { %>
		"html5shiv": "aFarkas/html5shiv#3.7.2"<% if (includejQuery || includeSizzle) {%>,<%}}} if (includejQuery) { %>
		"jquery": "components/jquery#1.11.2"<% } else { if (includeSizzle) { %>
		"sizzle": "jquery/sizzle#2.0.0"<% }} %>
	}
}